{
    "name": "your/namespace",
    "description": "Extended translations for Laravel Projects",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "languages", "lang", "translations", "messages"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "dragon-code/support": "^5.4",
        "laravel-lang/publisher": "^11.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\Translations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "YourNamespace\\Translations\\ServiceProvider"
            ]
        }
    }
}
