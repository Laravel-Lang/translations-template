{
    "name": "your/namespace",
    "description": "Extended translations for Laravel Projects",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "languages",
        "lang",
        "translations",
        "messages"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro"
        },
        {
            "name": "Laravel-Lang Team",
            "homepage": "https://github.com/Laravel-Lang"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel-lang/publisher": "^14.6"
    },
    "require-dev": {
        "laravel-lang/status-generator": "^1.13",
        "phpunit/phpunit": "^9.6",
        "symfony/var-dumper": "^6.2"
    },
    "conflict": {
        "laravel-lang/publisher": "<14.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "YourNamespace\\Translations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourNamespace\\Translations\\ServiceProvider"
            ]
        }
    }
}
